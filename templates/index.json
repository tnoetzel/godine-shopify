    "custom_liquid_JhRgja": {
      "type": "custom-liquid",
      "disabled": true,
      "custom_css": [],
      "settings": {
        "custom_liquid": "<style>\n    #home-hero {\n        background-image: url(\"https://cdn.shopify.com/s/files/1/0565/6087/7686/files/spring-25-mobile.jpg?v=1745860787\");\n        background-size: cover;\n        background-position: center bottom;\n        background-repeat: no-repeat;\n        color: white;\n        padding: 16px !important;\n        min-height: 180px;\n    }\n    \n\n    #home-hero h1 {\n        text-align: center;\n        color: white;\n        font-size: 3.2rem;\n        line-height: 1;\n        /* letter-spacing: -0.5px; */\n        text-shadow: 0 2px 4px rgba(0,0,0,0.80);\n        margin: 0;\n    }\n\n    #home-hero .btn-block {\n        margin-bottom: 24px;\n    }\n\n    #home-hero .btn-block a {\n        display: flex;\n        max-width: 200px;\n        margin: 8px auto;\n        text-align: center;\n        text-transform: uppercase;\n        box-shadow: none;\n        background-color: #DB4D59;\n        border-color: #DB4D59;\n        font-weight: 400;\n    }\n\n    #home-hero .btn-block a::after {\n        box-shadow: none;\n    }\n\n    @media screen and (min-width: 600px) {\n        #home-hero {\n            background-image: url(\"https://cdn.shopify.com/s/files/1/0565/6087/7686/files/spring-25-desktop.jpg?v=1745860787\");\n            padding: 24px !important;\n            display: flex;\n            justify-content: space-between;\n            min-height: 240px;\n        }\n\n        #home-hero h1 {\n            font-size: 3.6rem;\n        }\n\n        #home-hero .btn-block a {\n            width: 100%;\n            font-size: 2rem;\n            height: 36px;\n            max-width: 300px;\n        }\n    }\n\n    @media screen and (min-width: 960px) {\n        #home-hero {\n            padding: 32px !important;\n            min-height: 300px;\n        }\n\n        #home-hero h1 {\n            font-size: 5.6rem !important;\n        }\n\n        #home-hero .btn-block {\n            display: flex;\n            flex-direction: column;\n            justify-content: center;\n            align-items: center;\n            width: max-content;\n        }\n\n        #home-hero .btn-block a {\n            width: 100%;\n            font-size: 2rem;\n            height: 44px;\n        }\n    }\n\n    @media screen and (min-width: 1440px) {\n        #home-hero {\n            min-height: 480px;\n        }\n\n        #home-hero h1 {\n            font-size: 6.4rem !important;\n        }     \n\n        #home-hero .btn-block a {\n            font-size: 3.2rem;\n            height: 64px;\n        }\n    }\n</style>\n\n<section id=\"home-hero\">\n    <div class=\"hero-copy\">\n        <h1>See What&#8217;s New <br /> For Spring!</h1>\n    </div>\n\n    <div class=\"btn-block\">\n        <a href=\"/collections/category-hot-off-the-press\" class=\"button\">New Titles</a>\n        <a href=\"/collections/all\" class=\"button\">All Titles</a>\n    </div>\n</section>",
        "color_scheme": "",
        "padding_top": 0,
