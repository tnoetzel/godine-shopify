    "custom_liquid_wLQn3a": {
      "type": "custom-liquid",
      "name": "FALL 2025",
      "settings": {
        "custom_liquid": "<style>\n\n    #home-hero-fall-2025 {\n        display: flex;\n        flex-direction: row;\n        min-height: 600px;\n    }\n\n    #home-hero-fall-2025 .col-left {\n        flex: 1 0 40%;\n        color: #F0D489;\n        background-color: #8A1317;\n        color: white;\n        padding: 16px;\n        display: flex;\n        align-items: center;\n        justify-content: flex-end;\n    }\n\n    #home-hero-fall-2025 .col-right {\n        flex: 1 0 60%;\n        background-image: url(\"https://cdn.shopify.com/s/files/1/0565/6087/7686/files/fall_2025_banner.jpg?v=1759773641\");\n        background-size: cover;\n        background-position: center bottom;\n        background-repeat: no-repeat;\n        color: white;\n        padding: 16px !important;\n        min-height: 180px;\n        display: flex;\n        flex-direction: column;\n        justify-content: center;\n        gap: 16px;\n    }\n\n    #home-hero-fall-2025 h1 {\n        text-align: right;\n        color: #F0D489;\n        font-size: 24px;\n    }\n\n    #home-hero-fall-2025 .button {\n        max-width: 250px;\n        background-color: #F0D489;\n        border: 1px solid #F0D489;\n        color: #006958;\n        text-transform: uppercase;\n        letter-spacing: 2.5px;\n        font-weight: 800;\n    }\n\n    #home-hero-fall-2025 .button::after {\n        box-shadow: none;\n    }\n\n    .visible-0 {\n        display: block;\n    }\n    .visible-960 {\n        display: none;\n    }\n    .visible-1512 {\n        display: none;\n    }\n\n    @media screen and (min-width: 960px) {\n        #home-hero-fall-2025 h1 {\n            font-size: 60px;\n        }\n\n        #home-hero-fall-2025 .button {\n            font-size: 24px;\n        }\n\n        #home-hero-fall-2025 .col-left {\n            padding: 40px;\n        }\n\n        .visible-0 {\n            display: none;\n        }\n        .visible-960 {\n            display: block;\n        }\n    }\n\n    @media screen and (min-width: 1512px) {\n        #home-hero-fall-2025 h1 {\n            font-size: 80px;\n        }\n\n        .visible-960 {\n            display: none;\n        }\n        .visible-1512 {\n            display: block;\n        }\n    }\n</style>\n\n<section id=\"home-hero-fall-2025\">\n\n    <div class=\"col-left\">\n        <h1 class=\"visible-1512\">Jump into <br /> one of our new <br /> fall books.</h1>\n        <h1 class=\"visible-960\">Jump into <br /> one of our<br /> new fall <br /> books.</h1>\n        <h1 class=\"visible-0\">Jump into one of our new fall books.</h1>\n    </div>\n\n\n    <div class=\"col-right\">\n            <a href=\"/collections/category-hot-off-the-press\" class=\"button\">New Books</a>\n            <a href=\"/collections/all\" class=\"button\">All Books</a>\n    </div>\n</section>",
        "color_scheme": "",
        "padding_top": 0,
